  },
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronbeall/i18n-possessive.git"
  },
  "keywords": [
    "i18n",
    "possessive",
    "translation"
  ],
  "author": "Aaron Beall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaronbeall/i18n-possessive/issues"
  },
  "homepage": "https://github.com/aaronbeall/i18n-possessive#readme",
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
